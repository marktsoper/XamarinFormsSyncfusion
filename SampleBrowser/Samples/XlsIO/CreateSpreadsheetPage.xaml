<?xml version="1.0" encoding="utf-8" ?>
<local:SamplePage
             xmlns:local="clr-namespace:SampleBrowser;assembly=SampleBrowser"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SampleBrowser.CreateSpreadsheetPage">


  <local:SamplePage.ContentView>
    <StackLayout>

      <Label  x:Name="SampleTitle" Text = "Create Spreadsheet"
            FontSize="Large"
              HorizontalOptions="Center"
            VerticalOptions="CenterAndExpand">
      </Label>
      <Label></Label>
      <Label x:Name="Content_1" Text="This sample demonstrates the creation of a simple Excel document."
                 FontSize="Medium" VerticalOptions="CenterAndExpand">
      </Label>
      <Label></Label>
      <Label x:Name="Content_2" Text="Click “Generate Excel” button to save and view the Excel document generated by XlsIO."
                 FontSize="Medium" VerticalOptions="CenterAndExpand">
      </Label>
      <Label></Label>
      <Button x:Name="btnGenerate" Clicked ="OnButtonClicked" Text ="Generate Excel" HorizontalOptions="Center" VerticalOptions="CenterAndExpand"></Button>
    </StackLayout>
  </local:SamplePage.ContentView>
  <local:SamplePage.PropertyView>

  </local:SamplePage.PropertyView>

</local:SamplePage>
